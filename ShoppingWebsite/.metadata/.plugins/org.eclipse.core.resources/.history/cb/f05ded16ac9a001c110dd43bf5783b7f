package com.ecommerce.shoppingwebsite.dao;

import javax.persistence.FetchType;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;

import com.ecommerce.shoppingwebsite.entity.Category;


public class ProductDao {private Long id;
private String name;
private int category;
private double price;
private double weight;
private String description;
private String imageName;
public Long getId() {
	return id;
}
public void setId(Long id) {
	this.id = id;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public int getCategory() {
	return category;
}
public void setCategory(int category) {
	this.category = category;
}
public double getPrice() {
	return price;
}
public void setPrice(double price) {
	this.price = price;
}
public double getWeight() {
	return weight;
}
public void setWeight(double weight) {
	this.weight = weight;
}
public String getDescription() {
	return description;
}
public void setDescription(String description) {
	this.description = description;
}
public String getImageName() {
	return imageName;
}
public void setImageName(String imageName) {
	this.imageName = imageName;
}
@Override
public String toString() {
	return "ProductDao [id=" + id + ", name=" + name + ", category=" + category + ", price=" + price + ", weight="
			+ weight + ", description=" + description + ", imageName=" + imageName + "]";
}
public ProductDao(Long id, String name, int category, double price, double weight, String description,
		String imageName) {
	super();
	this.id = id;
	this.name = name;
	this.category = category;
	this.price = price;
	this.weight = weight;
	this.description = description;
	this.imageName = imageName;
}
public ProductDao() {
	super();
	// TODO Auto-generated constructor stub
}

}



